<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AlodokterMobileTest" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">@color/AloBlue</item>
    </style>

    <style name="Theme.AlodokterMobileTest.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AlodokterMobileTest.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AlodokterMobileTest.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>